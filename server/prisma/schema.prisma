datasource db {
  provider = "postgresql"
  url      = env("API_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        String   @id
  userId    String
  label     String
  done      Boolean
  createdAt DateTime
}

model item {
  userid    String
  id        Int      @id @default(autoincrement())
  itemname  String
  itemvalue Int
  createdAt DateTime
}
